{% extends 'admin/base.html.twig' %}
    {% block page %}



 <div class="col-sm-7 col-md-8 col-lg-8">
     <div class="jumbotron">
        {% block bodyTitle %}  <h3 class="h1">Create Advert </h3>   {% endblock %}
        {{ include('admin/_form.html.twig') }}
     </div>
 </div>
<div class="col-sm-5 col-md-3 col-lg-4">
        {% block sideBarRight %}
            <table class="table  table-hover">
                <tbody>
                {% for badge in badges %}
                    <tr>
                        <td> <a href="/admin/advert/edit/{{ badge.id }}"> {{ badge.name |capitalize }} </a></td>
                    </tr>
                {%  endfor %}
                </tbody>
            </table>
        {% endblock %}
</div>
    {% endblock %} 

 
  

    

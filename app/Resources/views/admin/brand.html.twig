{% extends 'admin/base.html.twig' %}
    {% block page %}
<div class="col-sm-9 col-md-8 col-lg-8">
    <div class="jumbotron">
        {% block bodyTitle %}  <h3 class="h1">Create Brand </h3>   {% endblock %}


        {% block content %}
          {{ include('admin/_form.html.twig') }}
        {% endblock %}

    </div>
</div>

        <div class="col-sm-3 col-md-3 col-lg-4">
            {% block sideBarRight %}
                <table class="table  table-hover">
                    <tbody>
                    {% for brand in brands %}
                        <tr>
                            <td> <a href="/admin/advert/edit/{{ brand.id }}"> {{ brand.name |capitalize }} </a></td>
                        </tr>
                    {%  endfor %}
                    </tbody>
                </table>
            {% endblock %}
        </div>

    {% endblock %} 

 
  

    
